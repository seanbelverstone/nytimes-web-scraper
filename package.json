{
  "name": "web-scraper",
  "version": "1.0.0",
  "description": "NY Times Web Scraping Website",
  "engines": {
    "node": "v11.13.0"
  },
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.17.1",
    "mongojs": "^2.6.0",
    "mongoose": "^5.7.5",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-icons": "^3.7.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.1.2",
    "reactstrap": "^8.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanbelverstone/nytimes-web-scraper.git"
  },
  "bugs": {
    "url": "https://github.com/seanbelverstone/nytimes-web-scraper/issues"
  },
  "homepage": "https://github.com/seanbelverstone/nytimes-web-scraper#readme"
}
